{
  "name": "react-starter",
  "version": "0.0.2",
  "author": "Artem Sitnikov <seestein9@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch": "yarn cert && webpack serve",
    "cert": "mkdir -p ./.cert && mkcert -cert-file ./.cert/localhost-cert.pem -key-file ./.cert/localhost-key.pem localhost 127.0.0.1 ::1",
    "build": "webpack --node-env production",
    "bundle-report": "yarn build --analyze",
    "release": "standard-version",
    "ts-type-check": "tsc --noEmit",
    "lint-js": "eslint --ext .ts --ext .tsx \"src/\" --quiet",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "prepare": "husky install"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .ts --ext .tsx \"src/\" --quiet"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.2",
    "classnames": "2.3.1",
    "history": "5.1.0",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "universal-router": "9.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/node": "7.16.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@types/assets-webpack-plugin": "7.1.0",
    "@types/classnames": "2.3.1",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-redux": "7.1.20",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "autoprefixer": "10.4.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "cache-loader": "4.1.0",
    "css-loader": "6.5.1",
    "cssnano": "5.0.10",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lint-staged": "11.2.6",
    "mini-css-extract-plugin": "2.4.5",
    "postcss": "8.3.11",
    "postcss-custom-media": "8.0.0",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.0",
    "postcss-nested": "5.0.6",
    "postcss-nesting": "10.2.0",
    "postcss-omit-import-tilde": "1.0.1",
    "postcss-partial-import": "4.1.0",
    "precss": "4.0.0",
    "prettier": "2.5.1",
    "prettier-stylelint": "0.4.2",
    "standard-version": "9.3.2",
    "style-loader": "3.3.1",
    "stylelint": "14.0.1",
    "stylelint-config-standard": "23.0.0",
    "typescript": "4.4.4",
    "webpack": "5.62.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
